model RolePermission {
  id            Int      @id @default(autoincrement())
  roleId        Int
  tableId       String
  // todo columns
  role          Roles    @relation(fields: [roleId], references: [id])
  table         TableMeta   @relation(fields: [tableId], references: [id])
  @@unique([roleId, tableId])
  @@map("role_permissions")
}